{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "memory": 512,
      "excludeFiles": [
        "venv/**",
        "__pycache__/**",
        "*.csv",
        "*.md",
        "models/**",
        "*.log"
      ]
    }
  }
}
